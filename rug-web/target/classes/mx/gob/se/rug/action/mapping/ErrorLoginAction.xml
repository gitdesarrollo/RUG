    "http://struts.apache.org/dtds/struts-2.0.dtd" >
    
<struts>
	<package name="errorlogin-action" namespace="/home" extends="base-action">
		<action name="errorlogin" method="errorlogin" class="errorloginAction">
			<result type="tiles">home.errorlogin</result>
			<result name="success" type="tiles">home.errorlogin</result>
			<result name="error" type="tiles">home.errorlogin</result>
		</action>
	</package>
</struts>
