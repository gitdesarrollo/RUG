<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd" >
   
<struts>
	<package name="garantia-action" namespace="/home" extends="base-action">
		<action name="garantia" method="garantia" class="garantiaAction">
			<result type="tiles">grals.home</result>
			<result name="success" type="tiles">garantia.home</result>
			<result name="error" type="tiles">garantia.home</result>
		</action>
		
		<action name="inscripcionGarantia" method="inscripcionGarantia" class="garantiaAction">
			<result type="tiles">grals.home</result>
			<result name="success" type="tiles">inscripcion.garantia.home</result>
			
		</action>
		
		<action name="detalleGarantia" method="detalleGarantia" class="garantiaAction">
			<result type="tiles">detalleGarantia.home</result>
			<result name="success" type="tiles">detalleGarantia.home</result>
			
		</action>
	</package>
	
	
</struts>